{
  "productName": "electron-vue",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tslint.json",
    "!vue.config.js",
    "!webpack.config.js",
    "!postcss.config.js",
    "!babel.config.js",
    "!eslintrc.js"
  ],
  "win": {
    "icon": "dist/assets/icons",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/assets/icons",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "target": [
      "AppImage"
    ]
  }
}
